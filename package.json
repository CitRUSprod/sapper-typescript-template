{
    "name": "sapper-typescript-template",
    "author": "CitRUS",
    "license": "MIT",
    "scripts": {
        "validate": "svelte-check --ignore src/node_modules/@sapper",
        "validate:dev": "svelte-check --ignore src/node_modules/@sapper --watch",
        "sapper:dev": "sapper dev",
        "sapper:build": "cross-env NODE_ENV=production sapper build --legacy",
        "dev": "run-p validate:dev sapper:dev",
        "prod": "run-s validate sapper:build",
        "start": "node __sapper__/build"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "9.0.0",
        "@rollup/plugin-replace": "2.3.3",
        "@rollup/plugin-typescript": "5.0.2",
        "@types/compression": "1.7.0",
        "@types/polka": "0.5.1",
        "cross-env": "7.0.2",
        "npm-run-all": "4.1.5",
        "prettier": "2.1.1",
        "prettier-plugin-svelte": "1.1.1",
        "rollup": "2.26.5",
        "rollup-plugin-svelte": "6.0.0",
        "rollup-plugin-terser": "7.0.0",
        "sapper": "0.28.2",
        "svelte": "3.24.1",
        "svelte-check": "1.0.25",
        "svelte-preprocess": "4.1.1",
        "tslib": "2.0.1",
        "typescript": "4.0.2"
    },
    "dependencies": {
        "compression": "1.7.4",
        "polka": "0.5.2",
        "sirv": "1.0.6"
    }
}
